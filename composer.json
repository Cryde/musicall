{
    "name": "cryde/musicall",
    "description": "MusicAll is a social network for music lover",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.1.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "^2.6",
        "composer/package-versions-deprecated": "1.11.99.1",
        "doctrine/annotations": "^1.0",
        "doctrine/dbal": "^2",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.8",
        "friendsofsymfony/jsrouting-bundle": "^3.1",
        "gesdinet/jwt-refresh-token-bundle": "^1.0",
        "google/apiclient": "^2.0",
        "google/apiclient-services": "^0.205.0",
        "lexik/jwt-authentication-bundle": "^2.6",
        "liip/imagine-bundle": "2.x-dev",
        "mailjet/mailjet-apiv3-php": "^1.4",
        "nelmio/cors-bundle": "^2.2",
        "nyholm/psr7": "^1.3",
        "phpdocumentor/reflection-docblock": "^5.2",
        "phpstan/phpdoc-parser": "^1.5",
        "sensio/framework-extra-bundle": "^6.0",
        "sentry/sdk": "^3.1",
        "sentry/sentry": "^3.1",
        "sentry/sentry-symfony": "^4.0",
        "symfony/asset": "^6.0",
        "symfony/console": "^6.0",
        "symfony/dotenv": "^6.0",
        "symfony/expression-language": "^6.0",
        "symfony/flex": "^2.1",
        "symfony/form": "^6.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/http-client": "^6.0",
        "symfony/intl": "^6.0",
        "symfony/mailer": "^6.0",
        "symfony/mime": "^6.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/process": "^6.0",
        "symfony/property-access": "^6.0",
        "symfony/property-info": "^6.0",
        "symfony/proxy-manager-bridge": "^6.0",
        "symfony/runtime": "^6.0",
        "symfony/security-bundle": "^6.0",
        "symfony/serializer": "^6.0",
        "symfony/string": "^6.0",
        "symfony/translation": "^6.0",
        "symfony/twig-bundle": "^6.0",
        "symfony/uid": "^6.0",
        "symfony/validator": "^6.0",
        "symfony/web-link": "^6.0",
        "symfony/webpack-encore-bundle": "^1.0",
        "symfony/yaml": "^6.0",
        "tgalopin/html-sanitizer-bundle": "^1.4",
        "vich/uploader-bundle": "^1.9"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.3",
        "phpstan/phpstan-doctrine": "^1.0",
        "phpstan/phpstan-symfony": "^1.0",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^6.0",
        "symfony/css-selector": "^6.0",
        "symfony/debug-bundle": "^6.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/stopwatch": "^6.0",
        "symfony/var-dumper": "^6.0",
        "symfony/web-profiler-bundle": "^6.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^6.0"
        }
    }
}
