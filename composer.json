{
    "name": "cryde/musicall",
    "description": "MusicAll is a social network for music lover",
    "type": "project",
    "version": "2.0.0",
    "license": "proprietary",
    "require": {
        "php": ">=8.4.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "^4.2.2",
        "doctrine/annotations": "^2.0.2",
        "doctrine/dbal": "^4.3.4",
        "doctrine/doctrine-bundle": "^2.18",
        "doctrine/doctrine-migrations-bundle": "^3.5.0",
        "doctrine/orm": "^3.5.2",
        "friendsofsymfony/jsrouting-bundle": "^3.5.2",
        "gesdinet/jwt-refresh-token-bundle": "^1.5.0",
        "google/apiclient": "^2.18.4",
        "google/apiclient-services": "^0.420.0",
        "jolicode/elastically": "^1.9.2",
        "league/flysystem-aws-s3-v3": "^3.29",
        "lexik/jwt-authentication-bundle": "^3.1.1",
        "liip/imagine-bundle": "^2.15",
        "longitude-one/doctrine-spatial": "^5.0.4",
        "mailjet/mailjet-apiv3-php": "^1.6.5",
        "nelmio/cors-bundle": "^2.5",
        "nyholm/psr7": "^1.8.2",
        "oneup/flysystem-bundle": "^4.12.4",
        "pentatrion/vite-bundle": "^8.2.2",
        "phpdocumentor/reflection-docblock": "^5.6.3",
        "phpstan/phpdoc-parser": "^2.3",
        "ramsey/uuid-doctrine": "^2.1",
        "runtime/frankenphp-symfony": "^0.2.0",
        "sentry/sdk": "^4",
        "sentry/sentry": "^4.16",
        "sentry/sentry-symfony": "^5.6",
        "symfony/ai-agent": "dev-main",
        "symfony/ai-platform": "dev-main",
        "symfony/asset": "^7.3.0",
        "symfony/brevo-mailer": "^7.3.2",
        "symfony/console": "^7.3.4",
        "symfony/dotenv": "^7.3.2",
        "symfony/expression-language": "^7.3.2",
        "symfony/flex": "^2.8.2",
        "symfony/form": "^7.3.4",
        "symfony/framework-bundle": "^7.3.4",
        "symfony/html-sanitizer": "^7.3.3",
        "symfony/http-client": "^7.3.4",
        "symfony/intl": "^7.3.4",
        "symfony/mailer": "^7.3.4",
        "symfony/mime": "^7.3.4",
        "symfony/monolog-bundle": "^3.10",
        "symfony/process": "^7.3.4",
        "symfony/property-access": "^7.3.3",
        "symfony/property-info": "^7.3.4",
        "symfony/psr-http-message-bridge": "^7.3.0",
        "symfony/runtime": "^7.3.4",
        "symfony/security-bundle": "^7.3.4",
        "symfony/serializer": "^7.3.4",
        "symfony/string": "^7.3.4",
        "symfony/translation": "^7.3.4",
        "symfony/twig-bundle": "^7.3.4",
        "symfony/uid": "^7.3.1",
        "symfony/validator": "^7.3.4",
        "symfony/web-link": "^7.3.0",
        "symfony/yaml": "^7.3.3",
        "vich/uploader-bundle": "^2.8.1"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.4",
        "deployer/deployer": "^7.5.12",
        "league/flysystem-memory": "^3.29",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpstan": "^2.1.31",
        "phpstan/phpstan-doctrine": "^2.0.10",
        "phpstan/phpstan-symfony": "^2.0.8",
        "phpunit/phpunit": "^12.4.1",
        "rector/rector": "^2.2.3",
        "symfony/browser-kit": "^7.3.2",
        "symfony/css-selector": "^7.3.0",
        "symfony/debug-bundle": "^7.3.4",
        "symfony/maker-bundle": "^1.64",
        "symfony/phpunit-bridge": "^7.3.4",
        "symfony/stopwatch": "^7.3.0",
        "symfony/var-dumper": "^7.3.4",
        "symfony/web-profiler-bundle": "^7.3.4",
        "zenstruck/foundry": "^2.7.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.0.0"
        }
    }
}
